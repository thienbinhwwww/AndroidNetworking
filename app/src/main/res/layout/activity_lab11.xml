<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#323232"
    tools:context=".lab1.Lab11Activity">

    <ImageView
        android:id="@+id/img_demo11Ac"
        android:layout_width="120dp"
        android:layout_height="101dp"
        android:layout_marginTop="132dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/tv_demo11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="51dp"
        android:text="Lab11"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/img_demo11Ac"
        app:layout_constraintStart_toStartOf="@+id/img_demo11Ac"
        app:layout_constraintTop_toBottomOf="@+id/img_demo11Ac" />

    <Button
        android:id="@+id/btn_demo11Ac"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="51dp"
        android:text="Button"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/tv_demo11"
        app:layout_constraintStart_toStartOf="@+id/tv_demo11"
        app:layout_constraintTop_toBottomOf="@+id/tv_demo11" />
</androidx.constraintlayout.widget.ConstraintLayout>